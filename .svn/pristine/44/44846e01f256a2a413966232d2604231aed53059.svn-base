package org.tibco.automation.tsc.objrepo;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.tibco.automation.shared.utilities.SharedMethods;

public class TSC_TCE extends SharedMethods{

	public WebElement btn_gnh_menu(WebDriver driver){
		return driver.findElement(By.xpath("//button[@id='gnh-menu-btn-products']"));
	}
			
	public WebElement tce(WebDriver driver){
		return driver.findElement(By.xpath("//span[text()='Cloud Events']"));
	}
	
	public WebElement btn_upload(WebDriver driver){
		return driver.findElement(By.xpath("//button[@name='upload']"));
	}
	
	public WebElement browseToUpload(WebDriver driver){
		return driver.findElement(By.xpath("//label[@id='iPaaSPrecreateDragDropBrowseOption']"));
	}
	
	public WebElement btn_push(WebDriver driver){
		return driver.findElement(By.xpath("//span[text()='Push']"));
	}
	
	public WebElement appstate_Running(WebDriver driver){
		return driver.findElement(By.xpath("//span[text()='Running']"));
	}
	
	public By appstate_Running = By.xpath("//span[text()='Running']");
	
	public WebElement scaleInstance(WebDriver driver){
		return driver.findElement(By.xpath("//span[text()='Scale instances']"));
	}	
	
	public WebElement scale_up(WebDriver driver){
		return driver.findElement(By.xpath("//div[@class='tc-indicators-caret-up-status-running tc-indicator-running-caret-up']"));
	}
	
	public By scale_up = By.xpath("//div[@class='tc-indicators-caret-up-status-running tc-indicator-running-caret-up']");
	
	public WebElement scale(WebDriver driver){
		return driver.findElement(By.xpath("//span[@class='lbl-status-running']"));
	}
	
	public By scale = By.xpath("//span[@class='lbl-status-running']");
	
	public WebElement errorMsg_FailedToScale(WebDriver driver){
		return driver.findElement(By.xpath("//div[contains(text(),'Failed to scale application')]"));
	}
	
	
	
	
	
}
